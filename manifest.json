{
  "manifest_version": 2,
  "name": "Kubernetes uwufier",
  "description": "Replace all Kubernetes reference with 'K(uwu)be'",
  "homepage_url": "https://github.com/TheRealBarenziah/kuwubernetes-browser-extension",
  "version": "0.1",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./substitute.js"]
    }
  ]
}
